{
    "name": "horde/dav",
    "description": "Horde library for WebDAV, CalDAV, CardDAV",
    "type": "library",
    "homepage": "https://pear.horde.org",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Jan Schneider",
            "email": "jan@horde.org",
            "role": "lead"
        }
    ],
    "version": "2.0.0",
    "time": "2017-10-27",
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.horde.org"
        }
    ],
    "require": {
        "php": "^5.4.1 || ^7",
        "pear-pear.horde.org/Horde_Auth": "^2",
        "pear-pear.horde.org/Horde_Core": "^2",
        "pear-pear.horde.org/Horde_Http": "^2",
        "pear-pear.horde.org/Horde_Stream": "^1.2",
        "pear-pear.horde.org/Horde_Translation": "^2.2",
        "ext-ctype": "*",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-iconv": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-pcre": "*",
        "ext-simplexml": "*",
        "ext-spl": "*"
    },
    "suggest": {
        "ext-curl": "*"
    },
    "replace": {
        "pear-pear.horde.org/Horde_Dav": "2.*",
        "pear-horde/Horde_Dav": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Horde_Dav": "lib/"
        }
    }
}
